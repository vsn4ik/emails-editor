{
    "private": true,
    "name": "@vsn4ik/emails-editor",
    "version": "1.0.0",
    "description": "Emails Editor",
    "license": "MIT",
    "repository": "vsn4ik/emails-editor",
    "files": [
        "/dist",
        "/src"
    ],
    "main": "dist/emails-editor.js",
    "main:umd": "dist/emails-editor.umd.js",
    "module": "src/emails-editor.js",
    "type": "module",
    "scripts": {
        "build": "rollup --config",
        "build:docs": "cp -rf ./dist ./docs/",
        "build:dev": "cp -rf ./dist ./tests/html/",
        "watch": "rollup --config --watch",
        "serve": "serve --listen 5000 --no-clipboard ./tests/html/",
        "docs:screenshots": "node ./docs/screenshots.js",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "7.12.13",
        "@babel/core": "7.12.13",
        "@babel/plugin-proposal-class-properties": "7.12.13",
        "@babel/runtime": "7.12.13",
        "@rollup/plugin-babel": "5.2.3",
        "@rollup/plugin-commonjs": "17.1.0",
        "@rollup/plugin-node-resolve": "11.1.1",
        "jest": "26.6.3",
        "jest-image-snapshot": "4.3.0",
        "postcss-url": "10.1.1",
        "puppeteer": "7.0.1",
        "rollup": "2.38.5",
        "rollup-plugin-postcss": "4.0.0",
        "serve": "11.3.2"
    }
}
